<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent" android:layout_height="match_parent">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Main Office:"
        android:id="@+id/textView2"
        android:textColor="#fc0303"
        android:textSize="25dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Guidance Office:"
        android:id="@+id/textView3"
        android:textColor="#fc0303"
        android:textSize="25dp"
        android:layout_below="@+id/mainoffice"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Attendance Office: "
        android:id="@+id/textView4"
        android:layout_below="@+id/go"
        android:layout_alignParentStart="true"
        android:textColor="#fc0303"
        android:textSize="25dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Athletics Office: "
        android:id="@+id/textView5"
        android:layout_below="@+id/ao"
        android:layout_alignParentStart="true"
        android:textColor="#fc0303"
        android:textSize="25dp" />



    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="502-485-8489"
        android:id="@+id/Ato"
        android:textSize="25dp"
        android:background="#00ffffff"
        android:textAlignment="center"
        android:layout_below="@+id/textView5"
        android:layout_alignStart="@+id/ao" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="502-485-8241"
        android:id="@+id/mainoffice"
        android:background="#00ffffff"
        android:textSize="25dp"
        android:textAlignment="center"
        android:layout_below="@+id/textView2"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="502-485-8503"
        android:id="@+id/go"
        android:textSize="25dp"
        android:background="#01ffffff"
        android:textAlignment="center"
        android:layout_below="@+id/textView3"
        android:layout_alignStart="@+id/mainoffice" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="502-485-8241"
        android:id="@+id/ao"
        android:textSize="25dp"
        android:background="#01ffffff"
        android:textAlignment="center"
        android:layout_below="@+id/textView4"
        android:layout_alignStart="@+id/go" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_menu_call"
        android:id="@+id/imageButton3"
        android:layout_alignTop="@+id/mainoffice"
        android:layout_toEndOf="@+id/mainoffice"
        android:layout_marginStart="21dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_menu_call"
        android:id="@+id/imageButton6"
        android:layout_alignTop="@+id/go"
        android:layout_alignStart="@+id/imageButton3" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_menu_call"
        android:id="@+id/imageButton5"
        android:layout_alignTop="@+id/ao"
        android:layout_alignStart="@+id/imageButton6" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_menu_call"
        android:layout_alignTop="@+id/Ato"
        android:layout_alignStart="@+id/imageButton5"
        android:id="@+id/imageButton7" />

</RelativeLayout>